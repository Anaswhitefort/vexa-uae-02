{
  "name": "hpp-portfolio",
  "description": "hpp portfolio",
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf .cache && yarn start",
    "develop": "nodemon --exec \"gatsby develop\"",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "lint": "eslint . --ext ts --ext tsx",
    "prettier-write": "prettier --write \"src/**/*.js\"",
    "release": "auto shipit",
    "start": "yarn develop",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "type-check": "tsc --pretty --noEmit"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn prettier-write",
      "git add"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*"
    ],
    "delay": "1500"
  },
  "prettier": {
    "bracketSpacing": true,
    "proseWrap": "always",
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.141",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
