    "gatsby-plugin-typescript": "^2.1.11",
    "graphql-typescript-definitions": "^0.17.3",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.1",
    "multer": "^1.4.2",
    "netlify-cms-backend-fs": "^0.4.4",
    "nodemon": "^1.19.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "typescript": "^3.6.3",
